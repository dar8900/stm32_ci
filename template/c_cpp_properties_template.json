{
    "env": {
        "myDefaultIncludePath": [
            "${workspaceFolder}"
        ]
    },
    "configurations": [
        {
            "name": "linux",
            "intelliSenseMode": "clang-x64",
            "includePath": [
                "${myDefaultIncludePath}"
            ],
            "cppStandard": "c++17",
            "compileCommands": "${workspaceFolder}/compile_commands.json",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}